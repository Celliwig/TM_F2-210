+#include "rtd-1296.dtsi"
+#include "rtd-1296-pinnata-common.dtsi"
+
+/ {
+	chosen {
+		linux,initrd-start = <ROOTFS_NORMAL_START>;
+		linux,initrd-end = <ROOTFS_NORMAL_END>;
