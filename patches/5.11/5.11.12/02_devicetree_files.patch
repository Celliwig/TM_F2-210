+
+	ehci@98013000 {
+		compatible = "Realtek,rtd129x-ehci";
+		xen,passthrough;
+	};
+
+	ohci@98013400 {
+		compatible = "Realtek,rtd129x-ohci";
+		xen,passthrough;
