+				rtk,memory-reserve = <
+					ION_MEDIA_HEAP_PHYS1
+					ION_MEDIA_HEAP_SIZE1
+					ION_MEDIA_HEAP_FLAG1
+					ION_MEDIA_HEAP_PHYS2
+					ION_MEDIA_HEAP_SIZE2
+					ION_MEDIA_HEAP_FLAG2>;
+			};
+		}; /* rtk,ion */
+
+		fb {
+			compatible = "Realtek,rtk-fb";
+			buffer-cnt = <3>;
+			resolution = <1920 1080>;
+			fps = <60>;
