+	 * RC6_TX = 6, RCA_TX = 7, PANASONIC_TX = 8, KONKA_TX=9
+	 */
+	irda@98007400 {
+		Realtek,irrx-protocol = <1>;
+		Realtek,irtx-protocol = <1>;
