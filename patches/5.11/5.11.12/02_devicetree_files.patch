+			      <0x0 ION_MEDIA_HEAP_PHYS2 0x0 ION_MEDIA_HEAP_SIZE2>;
+		};
+
+		Tee_reserved: tee@0 {
+			no-map;
+			reg = <0x0 0x10100000 0x0 0x00F00000>;
