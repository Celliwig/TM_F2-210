+	dwc3_u3host: rtk_dwc3_u3host@98013E00 {
+		compatible = "Realtek,dwc3";
+		xen,passthrough;
+
+		dwc3_u3host@981F0000 {
+			xen,passthrough;
+		};
+	};
+
+};
+
+/* Nodes under rtd-129x-usb.dtsi which need passthrough */
+&dwc3_drd_usb3phy {
+	xen,passthrough;
+};
+
+&dwc3_drd_usb2phy {
+	xen,passthrough;
