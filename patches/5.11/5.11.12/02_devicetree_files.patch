+	};
+
+	hdcptx@9800D000 {
+		compatible = "realtek,rtk129x-hdcptx";
+		reg = <0x9800d000 0x400>;
+		interrupts = <0 31 4>; /*gen Ri*/
+		xen,passthrough;
+	};
+
+	hdmirx@98034000 {
+		compatible = "Realtek,rtk-mipi-top";
+		reg = <0x98037000 0xE0>,
+			<0x98034000 0xF54>,
+			<0x98035F00 0x2C>,
+			<0x98037700 0x98>,
+			<0x98004000 0xF0>,
