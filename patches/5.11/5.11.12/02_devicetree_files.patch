+			0x0C 105 //KEY_LEFT //LEFT
+			0x4C 352 //KEY_OK //OK
+			0x0E 106 //KEY_RIGHT //RIGHT
+			0x08 412 //KEY_PREVIOUS //PREVIOUS
+			0x48 108 //KEY_DOWN //DOWN
+			0x09 407 //KEY_NEXT //NEXT
